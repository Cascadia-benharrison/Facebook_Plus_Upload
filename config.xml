<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.Cascadia.Facebook_Plus_Upload"
        versionCode="1" 
        version   = "1.0.0">
    <!-- versionCode is optional and Android only -->

    <!-- <icon src="icon.png" /> -->
    
    <name>Facebook_Plus_Upload</name>

    <description>
        Test for photo upload for StateTuned app.
    </description>

    <author href="www.cascadia.edu">
        Ben, Kuor, Casey, Jae
    </author>
    
    <!-- Permissions for accessing camera (PhoneGap Build handles this) -->
    <feature name="http://api.phonegap.com/1.0/camera"/>
    
    <!-- Version was 2.5.0 when first built, now need to specify the newest version of PhoneGap -->
    <preference name="phonegap-version" value="2.7.0" />
    
    <!-- FacebookConnect Plugin -->
    <gap:plugin name="FacebookConnect">
		<param name="APP_ID" value="169052836589416" />
	</gap:plugin>

</widget>